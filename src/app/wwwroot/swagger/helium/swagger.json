    "/api/Movies": {
      "get": {
        "tags": [ "Movies" ],
        "summary": "Search for Movies",
        "description": "Returns a JSON array of Movie documents",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "The term used to search by Movie title <br> ```valid: 3 - 50 characters```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "matrix"
            }
          },
          {
            "name": "genre",
            "in": "query",
            "description": "Returns a JSON array of Movie documents filtered by genre<br> ```valid: 3 - 50 characters```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "action"
            }
          },
          {
            "name": "year",
            "in": "query",
            "description": "Get movies by year <br>\n  ```valid: 1873 - currentYear + 5```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 2003
            }
          },
          {
            "name": "rating",
            "in": "query",
            "description": "Get movies with a rating >= rating <br>\n  ```valid: 0.0 - 10.0```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "number",
              "example": 6.5
            }
          },
          {
            "name": "topRated",
            "in": "query",
            "description": "Get top rated movies <br> ```valid: true or false```",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "boolean",
              "example": false,
              "default": false
            }
          },
