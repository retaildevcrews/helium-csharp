    "/api/movies/{movieId}": {
      "get": {
        "tags": [ "Movies" ],
        "summary": "Get Movie by ID",
        "description": "Get a single `Movie` document by movieId",
        "parameters": [
          {
            "name": "movieId",
            "in": "path",
            "description": "`valid: begins with tt; followed by 5 - 7 digits`<br><br>example: The Matrix",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            },
            "example": "tt0133093"
          }
        ],
        "responses": {
          "200": {
            "description": "`Movie`",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Movie"
                },
                "example": {
                  "id": "tt0133093",
                  "partitionKey": "3",
                  "movieId": "tt0133093",
                  "type": "Movie",
                  "title": "The Matrix",
                  "year": 1999,
                  "runtime": 136,
                  "rating": 8.7,
                  "votes": 1546282,
                  "totalScore": 13452653,
                  "genres": [ "Action", "Sci-Fi" ],
                  "roles": [
                    {
                      "order": 1,
                      "actorId": "nm0000206",
                      "name": "Keanu Reeves",
                      "birthYear": 1964,
                      "category": "Actor",
                      "characters": [ "Neo" ]
                    },
                    {
                      "order": 2,
                      "actorId": "nm0000401",
                      "name": "Laurence Fishburne",
                      "birthYear": 1961,
                      "category": "Actor",
                      "characters": [ "Morpheus" ]
                    },
                    {
                      "order": 3,
                      "actorId": "nm0005251",
                      "name": "Carrie-Anne Moss",
                      "birthYear": 1967,
                      "category": "Actress",
                      "characters": [ "Trinity" ]
                    }
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Invalid movieId",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "Invalid movieId"
                }
              }
            }
          },
          "404": {
            "description": "movieId not found",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string",
                  "example": "movieId not found"
                }
              }
            }
          }
        }
      }
    },
