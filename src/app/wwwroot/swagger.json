    "/api/movies": {
      "get": {
        "tags": [ "Movies" ],
        "summary": "Search Movies",
        "description": "Returns an array of `Movie` documents optionally filtered by query string parameters",
        "parameters": [
          {
            "name": "q",
            "in": "query",
            "description": "Filter by search on Movie.Title<br><br>`valid: 2 - 20 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "matrix"
            }
          },
          {
            "name": "genre",
            "in": "query",
            "description": "Filter by genre in Movie.Genres<br><br>`valid: 3 - 20 characters`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "action"
            }
          },
          {
            "name": "year",
            "in": "query",
            "description": "Filter by year<br><br>`valid: 1874 - currentYear + 5`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 2003
            }
          },
          {
            "name": "rating",
            "in": "query",
            "description": "Filter by Movie.Rating >= rating<br><br>`valid: 0.0 - 10.0`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "number",
              "example": 6.5
            }
          },
          {
            "name": "actorId",
            "in": "query",
            "description": "Filter by actorId in Movies.Roles<br><br>`valid: begins with nm; followed by 5 - 7 integer digits`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "string",
              "example": "nm0000206"
            }
          },
          {
            "name": "pageNumber",
            "in": "query",
            "description": "1 based page number<br><br>`valid: 1 - 10,000`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 1,
              "default": 1
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "Limit to pageSize documents<br><br>`valid 1 - 1000`",
            "required": false,
            "style": "form",
            "explode": true,
            "schema": {
              "type": "integer",
              "example": 10,
              "default": 100
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Array of `Movie` documents or empty array if not found",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
